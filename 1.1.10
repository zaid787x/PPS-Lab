m1=float(input("subject 1: "))
m2=float(input("subject 2: "))
m3=float(input("subject 3: "))
m4=float(input("subject 4: "))
m5=float(input("subject 5: "))
sum = m1 + m2 + m3 + m4 + m5
avg = sum / 5.0
if 90 <= avg <= 100:
	grade = 'A'
elif 80 <= avg <= 89:
	grade = 'B' 
elif 70 <= avg <= 79:
	grade = 'C'
elif 60 <= avg <= 69:
	grade = 'D'
else:
	grade = 'F'
print(f"Average Marks: {avg:.2f}")
print(f"Grade: {grade}")
